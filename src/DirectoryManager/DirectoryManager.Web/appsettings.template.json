{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },

  "Serilog": {
    "MinimumLevel": "Error",
    "WriteTo": [
      {
        "Name": "MSSqlServer",
        "Args": {
          "connectionString": "",
          "tableName": "LogEntries",
          "autoCreateSqlTable": false
        }
      }
    ]
  },

  "ConnectionStrings": {
    "DefaultConnection": ""
  },
  "ApplicationSettings": {
    "UseHttpsRedirection": true,
    "KestrelPort": 5001
  }
}